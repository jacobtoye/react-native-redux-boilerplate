{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver", {
            "root": ["./src"],
            "extensions": [".js", ".ios.js", ".android.js"],
            "alias": {
              "actions": "./actions",
              "components": "./components",
              "constants": "./constants",
              "containers": "./containers",
              "reducers": "./reducers",
              "sagas": "./sagas",
              "services": "./services",
              "screens": "./screens"
            }
          }
        ]
      ]
    }
  }
}
